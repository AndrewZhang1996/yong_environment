<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.ec.mapper.EcExergyFactorDetailMapper">

        <select id="getAllExergyFactor" resultType="org.jeecg.modules.ec.vo.EcExergyFactorDetailRespVo">
        select eefd.id, eefd.name, eefd.description
        from ec_exergy_factor_detail eefd
        order by eefd.id desc
    </select>


</mapper>