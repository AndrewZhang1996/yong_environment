<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.cq.mapper.CqMoistureRecordMapper">
    <select id="getRecordsByRoomId" resultType="org.jeecg.modules.cq.entity.CqMoistureRecord">
        select *
        from cq_moisture_content_record
        where room_id = #{roomId} order by record_time desc limit #{limit}
    </select>
</mapper>