<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.cq.mapper.CqUserDisplayInfoMapper">
    <select id="getUserDisplayInfoByUsername" resultType="org.jeecg.modules.cq.entity.CqUserDisplayInfo">
        select * from cq_user_display_info  where username = #{username}
    </select>
</mapper>